{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/.js": true,
        "**/.map": true
    },
    //Controls auto save of dirty { "**/*.extension": true }
    "files.autoSave": "afterDelay"
}